{
    "name": "guenbakku/cakeenv",
    "description": "Environment switch plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "cakephp/cakephp": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0",
        "mikey179/vfsstream": "1.6.4"
    },
    "autoload": {
        "psr-4": {
            "Guenbakku\\Cakeenv\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Guebakku\\Cakeenv\\Test\\": "tests"
        }
    }
}
